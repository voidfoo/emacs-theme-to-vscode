{
    "name": "Spacemacs-Light",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#000000",
        "editorCursor.background": "#000000",
        "editor.lineHighlightBackground": "#cccccc40",
        "editorLineNumber.foreground": "#999999",
        "editorLineNumber.activeForeground": "#000000",
        "editor.selectionBackground": "#cccccc",
        "editor.selectionHighlightBackground": "#dddddd60",
        "editor.findMatchBackground": "#ffff00",
        "editor.findMatchHighlightBackground": "#ffff0060",
        "titleBar.activeBackground": "#f0f0f0",
        "titleBar.activeForeground": "#000000",
        "activityBar.background": "#f0f0f0",
        "activityBar.foreground": "#0000ff",
        "sideBar.background": "#f0f0f0",
        "sideBar.foreground": "#000000",
        "statusBar.background": "#f0f0f0",
        "statusBar.foreground": "#000000",
        "input.background": "#f0f0f0",
        "input.foreground": "#000000",
        "input.placeholderForeground": "#808080",
        "diffEditor.insertedTextBackground": "#e6ffed60",
        "diffEditor.removedTextBackground": "#ffeef060",
        "diffEditor.insertedLineBackground": "#e6ffed40",
        "diffEditor.removedLineBackground": "#ffeef040",
        "editorBracketMatch.background": "#c0e8c3",
        "editorBracketMatch.border": "#ff0000",
        "editorGutter.background": "#ffffff",
        "editorGutter.modifiedBackground": "#97f295",
        "editorGutter.addedBackground": "#e6ffed",
        "editorGutter.deletedBackground": "#ffeef0"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#008000",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#800080"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#0000ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "meta.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.namespace",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#800080",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other",
                "variable.parameter",
                "variable.language",
                "support.variable"
            ],
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#000080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Error",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff0000"
            }
        }
    ],
    "semanticTokenColors": {
        "parameter": "#000080",
        "variable": "#000080",
        "property": "#000080",
        "function": "#0000ff",
        "method": "#0000ff",
        "class": "#800080",
        "enum": "#800080",
        "interface": "#800080",
        "typeParameter": "#800080",
        "type": "#800080",
        "comment": "#808080"
    }
}