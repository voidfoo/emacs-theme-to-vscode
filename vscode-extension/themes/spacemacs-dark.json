{
    "name": "Spacemacs-Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#000000",
        "editor.lineHighlightBackground": "#cccccc",
        "editorCursor.foreground": "#000000",
        "editorLineNumber.foreground": "#999999",
        "editorLineNumber.activeForeground": "#000000",
        "titleBar.activeBackground": "#f0f0f0",
        "titleBar.activeForeground": "#000000",
        "activityBar.background": "#f0f0f0",
        "activityBar.foreground": "#0000ff",
        "sideBar.background": "#f0f0f0",
        "sideBar.foreground": "#000000",
        "statusBar.background": "#f0f0f0",
        "statusBar.foreground": "#000000",
        "editor.selectionBackground": "#cccccc",
        "editor.selectionHighlightBackground": "#dddddd",
        "editor.wordHighlightBackground": "#dddddd"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#800080"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#0000ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#0000ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.namespace"
            ],
            "settings": {
                "foreground": "#800080",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "name": "Error",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff0000"
            }
        }
    ]
}